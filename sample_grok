    if "RESPMOD" in [message]{
        grok {
            match => {"message" => "\<%{INT:syslognum}\>%{SYSLOGTIMESTAMP:timestamp} %{SYSLOGHOST:deviceHostName} %{INT:randomnumber} %{TIMESTAMP_ISO8601:time} %{IP:deviceAddress} %{DATA:serviceName} %{INT:randonnumber} - RESPMOD:%{DATA:requestUrl} %{IP:sourceAddress} - %{GREEDYDATA:transactionUUID}"}
        }
    } else {
        grok {
            match => {"message" => "\<%{INT:syslognum}\>%{SYSLOGTIMESTAMP:timestamp} %{SYSLOGHOST:deviceHostName} %{INT:randomnumber} %{TIMESTAMP_ISO8601:time} %{IP:deviceAddress} %{DATA:serviceName} %{INT:randonnumber} - %{DATA:requestUrl} verdict %{DATA:verdict}. Hash: %{DATA:hash} Tenant: %{DATA:tenant} Transaction: %{GREEDYDATA:transactionUUID}"}
        }
    }
